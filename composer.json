{
    "name": "eslf/vanhaex",
    "version": "2.1.1",
    "description": "Easy, Simple and Lightweight web Framework for creating web applications",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "vanhaex"
        }
    ],
    "minimum-stability": "stable",
    "require": {
      "php": "^7.4",
      "phpmailer/phpmailer": "^6.4",
      "ext-mysqli":"*",
      "ext-json": "*",
      "smarty/smarty": "^4",
      "vanhaex/esdbaccess": "^1.0",
      "psr/http-server-middleware": "^1.0",
      "symfony/console": "^5.4",
      "ua-parser/uap-php": "^3.9"
    },
    "require-dev": {
        "vimeo/psalm": "^5.8",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Framework\\": "framework/",
            "Config\\": "config/",
            "App\\": "app/"
        }
    },
    "scripts": {
        "psalm": "./vendor/bin/psalm"
    }
}